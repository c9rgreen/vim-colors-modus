; Definitions common to all variants {{{
Variant: gui
#if get(g:, '@optionprefix_transp_bg', 0) && !has('gui_running')
; Variant: 256 8
Variant: 256 
#if get(g:, '@optionprefix_transp_bg', 0)
; Variant: gui 256 8
Variant: gui 256 
    Normal           fg_main             none
    Terminal         fg_main             none
#else
    Normal           fg_main             bg_main
    Terminal         fg_main             bg_main
#endif

